		"userdoc": {
			"methods": {
				"approve(address,uint256)": {
					"notice": "Set allowance for other address\r      * Allows `_spender` to spend no more than `_value` tokens on your behalf\r "
				},
				"approveAndCall(address,uint256,bytes)": {
					"notice": "Set allowance for other address and notify\r      * Allows `_spender` to spend no more than `_value` tokens on your behalf, and then ping the contract about it\r "
				},
				"burn(uint256)": {
					"notice": "Destroy tokens\r      * Remove `_value` tokens from the system irreversibly\r "
				},
				"burnFrom(address,uint256)": {
					"notice": "Destroy tokens from other account\r      * Remove `_value` tokens from the system irreversibly on behalf of `_from`.\r "
				},
				"transfer(address,uint256)": {
					"notice": "Transfer tokens\r      * Send `_value` tokens to `_to` from your account\r "
				},
				"transferFrom(address,address,uint256)": {
					"notice": "Transfer tokens from other address\r      * Send `_value` tokens to `_to` on behalf of `_from`\r "
				}
			}
